conf t
!
hostname ham-rtr
ip domain-name cit325.com
ip ssh version 2
username student privilege 15 secret Passw0rd!
crypto key generate rsa modulus 2048
!
interface GigabitEthernet0/0
 description Link to mgmt-rtr
 ip address 10.10.10.3 255.255.255.0
 no shut
!
interface GigabitEthernet0/1
 description Link link to reg-rtr
 ip address 15.15.15.2 255.255.255.0
 no shut
!
interface GigabitEthernet0/2
 description Link to ham-sw
 ip address 11.11.11.1 255.255.255.0
 no shut
!
router ospf 100
 network 10.10.10.0 0.0.0.255 area 0
 network 11.11.11.0 0.0.0.255 area 0
 network 15.15.15.0 0.0.0.255 area 0

line vty 0 4
 login local
 transport input ssh
 exec-timeout 10 0
!
no scheduler allocate
!
end
write memory
