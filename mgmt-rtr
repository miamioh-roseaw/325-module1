en
conf t
hostname mgmt-rtr
ip domain lookup
ip name-server 8.8.8.8
!
interface g0/0
 description Link to NAT
 ip address dhcp
 ip nat outside
 no shutdown
!
interface g0/1
 description mgmt plane
 ip address 10.10.10.1 255.255.255.0
 ip nat inside
 no shutdown
!
ip dhcp excluded-address 10.10.10.1 10.10.21.9
ip dhcp pool MGMT-PLANE
 network 10.10.10.0 255.255.255.0
 default-router 10.10.10.1
 dns-server 10.10.10.1
!
ip dhcp pool jenkins
   host 10.10.10.10 255.255.255.0
   client-identifier 0111.1111.1111.11 !change your jenkins-server's MAC address to 11:11:11:11:11:11
   default-router 10.10.10.1
    dns-server 10.10.10.1
!
ip dhcp pool ham-l
   host 10.10.10.11 255.255.255.0
   client-identifier 0111.1111.1111.12 !change  your ham-l's MAC address 11:11:11:11:11:12
   default-router 10.10.10.1
    dns-server 10.10.10.1
!
ip dhcp pool mid-l
   host 10.10.10.12 255.255.255.0
   client-identifier 0111.1111.1111.13 !change your mid-l's MAC address 11:11:11:11:11:13
   default-router 10.10.10.1
    dns-server 10.10.10.1
!
ip dhcp pool oxf-l
   host 10.10.10.13 255.255.255.0
   client-identifier 010c.d02e.a000.00 !change your oxf-l's MAC address, preceeded by type 01 0c:do:2e:a0:00:00
   default-router 10.10.10.1
    dns-server 10.10.10.1
!
ip dhcp pool mid-w
   host 10.10.10.14 255.255.255.0
   client-identifier 0111.1111.1111.15 !change your mid-w's MAC address 11:11:11:11:11:15
   default-router 10.10.10.1
    dns-server 10.10.10.1
!
ip dhcp pool ham-w
   host 10.10.10.15 255.255.255.0
   client-identifier 0111.1111.1111.16  !change your ham-w's MAC address to 11:11:11:11:11:16
   default-router 10.10.10.1
    dns-server 10.10.10.1
!
ip nat inside source list 1 interface g0/0 overload
access-list 1 permit 10.10.10.0 0.0.0.255
ip route 0.0.0.0 0.0.0.0 dhcp
end
