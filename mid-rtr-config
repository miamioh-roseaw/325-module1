conf t
hostname mid-rtr
ip domain name cit325.com
ip ssh version 2
username student privilege 15 secret Passw0rd!
line vty 0 4
transport input ssh
interface G0/0
 description Link mgmt-rtr
 ip address 10.10.10.1 255.255.255.0
 no shut
interface G0/2
 description Link to mid-sw
 ip address 12.12.12.1 255.255.255.0
 no shut
interface G0/1
 description Link to reg-rtr
 ip address 14.14.14.1 255.255.255.0
 no shut
 router ospf 100
  network 10.10.10.0 0.0.0.255 area 0
  network 12.12.12.0 0.0.0.255 area 0
  network 14.14.14.0 0.0.0.255 area 0
end
copy run start
startup-config
conf t
crypto key generate rsa                   ! use 768 when prompted
